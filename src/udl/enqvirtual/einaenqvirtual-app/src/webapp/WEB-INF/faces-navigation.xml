<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	<navigation-rule>
		<from-view-id>*</from-view-id>
			
			<navigation-case>
			<from-outcome>getInici</from-outcome>
			<to-view-id>/einaenqvirtual/index.jsp</to-view-id>
			<redirect/>
			</navigation-case>									
			 
			<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/einaenqvirtual/error.jsp</to-view-id>
			<redirect/>
			</navigation-case>
			
			<navigation-case>
			<from-outcome>inici</from-outcome>
			<to-view-id>/einaenqvirtual/index.jsp</to-view-id>			
			</navigation-case>
			
			<navigation-case>
			<from-outcome>presentacio</from-outcome>
			<to-view-id>/einaenqvirtual/presentacio.jsp</to-view-id>
			<redirect/>
			</navigation-case>
			
	</navigation-rule>
</faces-config>


